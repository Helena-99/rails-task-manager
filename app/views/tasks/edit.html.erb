<h1>Edit Task</h1>
<%=form_with model: @task do |f|%>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :details %>
<%= f.text_field :details %>
<div>
<%= f.label :completed, "Completed?" %>
<%= f.check_box :completed %>
</div>
<%= f.submit %>
<% end %>
<%= link_to "back to list", tasks_path %>
